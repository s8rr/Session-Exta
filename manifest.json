{
    "manifest_version": 2,
    "name": "Session Saver",
    "version": "1.0",
    "description": "Save and import browser sessions seamlessly.",
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  